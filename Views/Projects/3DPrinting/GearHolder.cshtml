@using System;
@using Helium24.Models;

@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<Project>
@model Helium24.Models.Project

@{
    Layout = "Layouts/RootLayout.cshtml";
}

@section Title {@Model.Title}

<div class="card">
    @Html.Partial("../Common/CardHeader.cshtml", Model)
    <div class="card-body">
        @Html.Partial("../Common/ImageCard.cshtml",
            new Image("/Blobs/Projects/3DPrinting/GearHolder/gear_holder.jpg", "Completed Gear Holder 3D print"))
        <br />
        @Html.Partial("../Common/ParagraphStart.cshtml")
        Available from <a href="http://www.thingiverse.com/thing:11071">Thingiverse</a>, this model cannot be produced
        with traditional manufacturing techniques. The gears (spares for the printer) cannot physically fit through the holes in the model.
        @Html.Partial("../Common/ParagraphEnd.cshtml")
        @Html.Partial("../Common/ParagraphStart.cshtml")
        This print was performed at thie highest resolution my printer can handle, 0.1 mm.
        @Html.Partial("../Common/ParagraphEnd.cshtml")
        @Html.Partial("../Common/ParagraphStart.cshtml")
        Print time was 3 hours.
        @Html.Partial("../Common/ParagraphEnd.cshtml")
        <br />
        @Html.Partial("../Common/ImageCard.cshtml",
            new Image("/Blobs/Projects/3DPrinting/GearHolder/bad_holder_corner.jpg", "The underside of the wireframe solid, showing bad streamers"))
        <br />
        @Html.Partial("../Common/ImageCard.cshtml",
            new Image("/Blobs/Projects/3DPrinting/GearHolder/good_holder_corner.jpg", "The upperside of the wireframe solid, with good layering"))
    </div>
</div>