@using H24.Models
@model H24.Models.IProject

<div class="card-header">
    <div class="row justify-content-between">
        <div class="col-auto align-self-center">
            @Model.Title
        </div>
        <div class="col-auto" style="flex: unset;">
            <div class="row">
                <partial name="Base/TagButtons.cshtml" model="@Model.Tags" />
                <div class="col-auto align-self-center">
                    <span class="text-muted"><partial name="Base/FormattedDate.cshtml" model="@Model.Date" /></span>
                </div>
            </div>
        </div>
    </div>
</div>